package main;

import java.awt.Dimension;

import javax.swing.JButton;
import javax.swing.JToolBar;

import metaSema.OtvoriEditor;

public class MyToolBar extends JToolBar {

	public MyToolBar() {
		JButton editor = new JButton("Editor");
		JButton navigacija = new JButton("Navigation");
		JButton dodaj = new JButton("Add");
		JButton izbrisi = new JButton("Delete");
		JButton update = new JButton("Update");
		JButton pretrazi = new JButton("Search");
		
		editor.addActionListener(new OtvoriEditor());
		
		add(navigacija);
		add(editor);
		addSeparator(new Dimension(5,5));
		add(dodaj);
		add(izbrisi);
		add(update);
		add(pretrazi);
		setFloatable(false);
	}
}
